        {
            // Web >Artist - Title
            "title": "Web Browser",
            "enabled": "true",
            "icon": "fa fa-globe",
            "window": {
                "class": "MozillaWindowClass",
                "title": "^\\u25B6\\s?(.* - .*)"
            },
            "regex": {
                "artist": "(.*?) - .*",
                "track": ".*? - (.*)"
            }
        },
        {
            // Web >Title by Artist
            "title": "Web Browser",
            "enabled": "true",
            "icon": "fa fa-globe",
            "window": {
                "class": "MozillaWindowClass",
                "title": "^\\u25B6\\s?(.* by .*)"
            },
            "regex": {
                "artist": ".* by (.*)",
                "track": "(.*) by .*"
            }
        }
    ],

    "replace": {
        "all": [
            // Special characters
            [ "[\\´\\’]", "'" ],
            [ "[\\“\\”]", "\"" ],

            // Featuring
            [ "(?i)featuring|feat\\.|ft\\.|f/|w/", "Feat." ],
            [ "(?i)\\(with ([^()]+)\\)", "(Feat. \\1)" ],   // with to feat
            [ "(?i)Feat\\.([^\\s]+)", "Feat. \\1" ],        // enforce a space
            [ "(?i)\\[(Feat\\. [^\\]]+)\\]", "(\\1)" ],     // brackets to parenthases

            // Spelling

            // Cuss words
            [ "(?i)B[i\\*][t\\*][c\\*][h\\*]", "bitch" ],
            [ "(?i)D[i\\*][c\\*][k\\*]", "dick" ],
            [ "(?i)F[u\\*][c\\*][k\\*][e\\*][d\\*]", "fucked" ],
            [ "(?i)F[u\\*][c\\*][k\\*][i\\*][n\\*]", "fuckin" ],
            [ "(?i)F[u\\*][c\\*][k\\*]", "fuck" ],
            [ "(?i)N[i\\*][g\\*][g\\*][a\\*][s\\*]", "niggas" ],
            [ "(?i)N[i\\*][g\\*][g\\*][a\\*]", "nigga" ],
            [ "(?i)P[u\\*][s\\*][s\\*][y\\*]", "pussy" ],
            [ "(?i)S[h\\*][i\\*][t\\*]", "shit" ]
        ],
        "artist": [
            // Suffixes
            [ "(?i)(.+), The$", "The \\1" ],
            [ "(?i)The The", "The" ],
            [ "(?i)(.+) \\((Pop|Pop Punk|Rock)\\)", "\\1" ]     // Pandora

            // Specific
        ],
        "track": [
            // Featuring
            [ "(?i)(Feat\\. [^()]+) of [^()]+", "\\1" ],   // "of" strip
            [ "(?i) - (Feat\\. [^()]+)", " (\\1)" ],       // Spotify hyphen fix
            [ "(?i) - (With\\. [^()]+)", " (\\1)" ],       // Spotify hyphen fix
            [ "(?i) (Feat\\. [^()]+)", " (\\1)" ],         // enforce parenthases

            // Suffixes
            [ "(?i) - (Mozilla Firefox)$", "" ],
            [ "(?i) - ([0-9]{4}|Acoustic.*|Airplay|Album Version|Best Of|Bonus.*|Clean|.*Cover|Dirty|.*Edit|Edited|Explicit|From .+|Full Length|Hidden Track|Live From .+|Main|.*Mix|Non-Album Track|Original|Original Version|Recorded at .+|.*Remaster|Remastered|.*Remastered [0-9]{2,4}|.*Remix|Radio|Single|Spotify Sessions.*|Unplugged|.*Version)$", "" ],
            [ "(?i) \\(([^()]+)?([0-9]{2,4} Version|Acoustic.*|Album|Amended|ASOT [0-9]+|Bonus|Clean|.+ Cover|Dirty|Edit|Edited|Explicit|From.+Soundtrack|Mainstream|Mix|Mix Cut|PG13|Prod|Radio|Remastered|Re-Recorded|Single)([^()]+)?\\)$", "" ],
            [ "[\\s\\-]+$", "" ]

            // Specific
        ],
        "album": []
    }
}